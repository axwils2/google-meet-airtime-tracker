{
  "manifest_version": 3,
  "name": "Video Call Air Time Monitor",
  "version": "0.2.0",
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": ["jquery-3.6.0.min.js", "content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
